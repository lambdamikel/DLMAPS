(in-package :racer-user)(full-reset)(with-unique-name-assumption (in-knowledge-base test test-abox) (define-primitive-attribute f) (define-primitive-attribute f-inv :inverse f) (define-primitive-attribute g :parent f) (define-primitive-attribute g-inv :parent f-inv :inverse g) (related i1 i2 f) (instance i2 (at-least 1 g-inv)) (test-assert (equal (individual-fillers i2 g-inv) '(i1))) )(in-knowledge-base test atest)(related a b r)(instance a bottom)(individuals-related? a b r)(with-unique-name-assumption (in-knowledge-base test test-abox) (define-primitive-attribute f) (define-primitive-attribute g :parent f) (related i1 i2 f) (instance i1 (at-least 1 g)) (test-assert (equal (individual-fillers i1 g) '(i2))) )