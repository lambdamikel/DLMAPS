(in-tbox told-value)(define-concrete-domain-attribute age :type integer)(define-concrete-domain-attribute name :type string)(in-abox told-value)(instance alice (and (an age) (a name)))(instance betty (and (an age)))(instance charles (and (a name)))(constrained alice alice-age age)(constrained alice alice-name name)(constrained alice huhu age)(constrained betty betty-age age)(constrained charles charles-name name);;;;;;;;;(constraints (string= charles-name "Charles"))(constraints (equal alice-age 80))(constraints (string= alice-name "Alice"))